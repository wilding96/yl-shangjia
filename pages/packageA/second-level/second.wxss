.entry .bgicon {
  width: 100%;
  display: block;
}
.type-navbar{
   white-space: nowrap; 
  background-color: #fff;
}
.type-navbar-item{
  border-bottom: 4rpx solid #fff;
  color: #999;
  width: 110rpx;
  margin: 0 10px;
}
.type-box{
  box-sizing: content-box;
  font-size: 24rpx;
  line-height: 84rpx;
  text-align: center;
  display: inline-block;
  overflow: hidden;
}
.entry .category {
  width: 100%;
  height: 35px;
  line-height: 35px;
  color: #3a3a3a;
  font-size: 13px;
  background-color: #fff;
  text-align: center;
  position: fixed;
  overflow-x: scroll;
  top: 0;
  z-index: 1000;
}

.entry .category .cate {
  display: inline-block;
  width: 70px !important;
  height: 34px;
  color: #999;
}

.entry .select {
  color: #B34852 !important;
  border-bottom: 3px solid #B34852;
}

.entry .con {
  background-color: #fff;
  padding: 0 15px;
}

.entry .con .yu {
  padding-top: 10px;
}

.entry .con .yu .logo {
  width: 70px;
  height: 70px;
  border-radius: 70px;
  vertical-align: middle;
}

.entry .con .yu .tip {
  font-family: PingFangSC-Light;
  font-size: 12px;
  color: #666666;
  display: inline-block;
  width: 70%;
  vertical-align: middle;
}

.entry .con .shelf {
  background: #B34852;
  height: 41px;
  line-height: 41px;
  color: #fff;
  text-align: center;
  font-family: FZLTHK--GBK1-0;
  font-size: 14px;
  margin: 20px 0 10px;
}

.entry .con .product {
  width: 100%;
  padding-bottom: 40px;
}

.entry .con .product .lists {
  display: -webkit-box;
}

.entry .con .product .list {
  display: block;
  -webkit-box-flex: 1;
  width: 1%;
  vertical-align: top;
  padding: 10px 5px 0;
}

.entry .con .product .list .imgs {
  position: relative;
  background-color: #f2f2f2;
}

.entry .con .product .list .imgs .pimg {
  width: 100%;
  margin: 0 auto;
  display: block;
}

.entry .con .product .list .imgs .peo {
  position: absolute;
  top: 0;
  left: 0;
  height: 44px;
  width: 44px;
  border-radius: 44px;
  background-color: #fff;
  border: 0.5px solid #333;
  color: #333333;
  text-align: center;
}

.entry .con .product .list .imgs .peo .count {
  font-family: DINMittelschriftStd;
  font-size: 11px;
  height: 11px;
  display: block;
  margin-top: 10px;
}

.entry .con .product .list .imgs .peo .ptext {
  font-family: PingFangSC-Regular;
  font-size: 8px;
  height: 8px;
  display: block;
}

.entry .con .product .list .manfact {
  height: 20px;
  line-height: 20px;
  background: #86CB41;
  font-family: PingFangSC-Regular;
  font-size: 10px;
  color: #FFF;
  padding: 0 8px;
  margin: 9px auto 0;
  display: inline-block;
}

.entry .con .product .list .title {
  overflow: hidden;
  padding-top: 10px;
}

.entry .con .product .list .title .tag {
  border: 1px solid #D43C33;
  border-radius: 1px;
  height: 16px;
  line-height: 16px;
  font-size: 10px;
  color: #D43C33;
  margin-right: 6px;
  padding: 0 3px;
  display: inline-block;
  vertical-align: middle;
}

.entry .con .product .list .title .t {
  font-family: PingFangSC-Regular;
  font-size: 15px;
  color: #333;
  line-height: 20px;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.entry .con .product .list .subtitle {
  font-family: PingFangSC-Regular;
  font-size: 11px;
  color: #888;
  line-height: 16px;
  display: block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.entry .con .product .list .price {
  height: 35px;
  line-height: 35px;
}

.entry .con .product .list .price .nowp {
  font-family: PingFangSC-Medium;
  font-size: 16px;
  color: #333333;
}

.entry .con .product .list .price .zp {
  font-family: PingFangSC-Regular;
  font-size: 10px;
  color: #B34852;
}

.entry .con .product .list .eventshelf {
  width: 95%;
  height: 32px;
  line-height: 32px;
  text-align: center;
  background: #B34852;
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #FFFFFF;
  border: 1px solid #B34852;
}

.entry .con .product .list .hui {
  background-color: #fff;
  color: #B34852;
}

