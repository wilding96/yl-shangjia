.page .header-tabs {
  opacity: 0.89;
  background: #FFFFFF;
  height: 40px;
  line-height: 40px;
  font-size: 13px;
  display: -webkit-box;
  color: #888888;
  width: 100%;
  position: fixed;
  z-index: 10000;
}
.xg {
  height: 50px;
}
.page .header-tabs .tabs {
  text-align: center;
  display: block;
  -webkit-box-flex: 1;
}

.page .header-tabs .tab-select {
  color: #B34852;
  font-size: 14px;
}

.page .noorder {
  position: absolute;
  text-align: center;
  top: 35%;
  width: 100%;
  color: #c5c5c5;
  font-size: 14px;
}

.page .noorder img {
  width: 80px;
}

.page .loading {
  color: #c5c5c5;
  text-align: center;
  font-size: 12px;
  padding: 30px 0;
  background: #fff;
}

.page {
  background-color: #f5f5f5;
}

.page .lists {
  background: #fff;
  margin-top: 50px;
  padding: 10px 15px;
}

.page .lists {
  margin: 0 auto;
}

.page .list {
  margin-bottom: 10px;
  width: 100%;
  background: #F77F7A;
  border-radius: 4px;
  color: #fff;
  position: relative;
}

.page .list .cd {
  padding: 23px 0;
}

.page .list .line {
  background: url("http://cdn.youlianyc.com/image/static/c842e87560d72a632883bd783fe3e7333aa00562.jpg!1125p") 0 0 repeat-x;
  background-size: 9px;
  height: 5px;
}

.page .list .tranfer {
  background: rgba(0, 0, 0, 0.1);
  color: #fff;
  padding: 8px 0;
  text-align: center;
  font-size: 12px;
}

.page .list .tranfer .topicon {
  height: 5px;
}

.page .list .gouser {
  background: #fff;
  border-radius: 2px;
  width: 56px;
  height: 21px;
  line-height: 21px;
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #F77F7A;
  text-align: center;
  position: absolute;
  right: 10px;
  top: 22px;
  border: 1px solid #F77F7A;
}

.page .list .zeng {
  border: 1px solid #fff;
  background: #F77F7A;
  color: #fff;
  top: 50px;
}

.page .list .left {
  display: inline-block;
  vertical-align: middle;
  width: 60%;
}

.page .list .title {
  font-family: PingFangSC-Regular;
  font-size: 14px;
}

.page .list .subtitle {
  font-size: 12px;
  line-height: 26px;
  width: 70%;
  overflow: hidden;
}

.page .list .end_time {
  font-family: PingFangSC-Regular;
  font-size: 10px;
  color: #FFFFFF;
  line-height: 25px;
}

.page .list .end_time .touse {
  font-size: 12px;
  color: #B34852;
  vertical-align: middle;
}

.page .list .end_time .used {
  border: 1px solid #3A3A3A;
  border-radius: 2px;
  font-size: 11px;
  color: #3A3A3A;
  padding: 2px 5px;
  margin-left: 7px;
  vertical-align: middle;
}

.page .list .end_time .expired {
  border: 1px solid #888;
  border-radius: 2px;
  font-size: 11px;
  color: #888;
  padding: 2px 5px;
  margin-left: 7px;
  vertical-align: middle;
}

.page .list .free_pay {
  display: inline-block;
  font-family: PingFang-SC-Medium;
  font-size: 48px;
  color: #FFFFFF;
  vertical-align: middle;
  width: 35%;
  text-align: center;
}

.page .list .free_pay .doller {
  font-size: 24px;
}

.page .used {
  background: #B8BBC3;
  border-radius: 4px;
}

.page .used .title, .page .used .subtitle, .page .used .end_time, .page .used .free_pay, .page .used .free_pay label {
  color: #fff;
}

.page .expired {
  background: #B8BBC3;
  border-radius: 4px;
}

.page .expired .title, .page .expired .subtitle, .page .expired .end_time, .page .expired .free_pay, .page .expired .free_pay label {
  color: #fff;
}
