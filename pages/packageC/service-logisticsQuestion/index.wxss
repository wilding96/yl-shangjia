.my {
  padding-bottom: 48px;
}
.my .order {
  background-color: #fff;
  color: #3A3A3A;
  margin-bottom: 10px;
}
.my .order .helpq {
  height: 40px;
  line-height: 40px;
  font-size: 16px;
  padding: 0 16px;
  border-bottom: 1px solid #eee;
  font-family: PingFangSC-Regular;
  color: #3a3a3a;
}
.my .order .manage {
  line-height: 24px;
  font-size: 14px;
  border-bottom: 1px solid #e5e4e3;
  padding: 10px 16px;
  background-color: #fff;
  display: block;
  color: #3a3a3a;
  position: relative;
}
.my .order .manage .olink {
  position: absolute;
  right: 16px;
  top: 12px;
  width: 8px;
  vertical-align: middle;
}
.my .order .manage .keeper {
  color: #999;
  font-size: 12px;
}
.my .order .fixed-manage{
  position:absolute;
  top:0;
  left:0;
  width:100%;
  height:100%;
}
.my .order .fixed-manage button{
  width: 100%;
  height: 100%;
  opacity: 0;
}
.my .order .ostatus {
  display: -webkit-box;
  height: 89px;
  padding: 0 16px;
}
.my .order .ostatus .s {
  -webkit-box-flex: 1;
  text-align: center;
  font-size: 12px;
  color: #3A3A3A;
  margin: 20px 0;
  display: block;
}
.my .order .ostatus .s img {
  width: 18px;
}
