.cart {
  padding-bottom: 100px;
  background: #f5f5f5;
  min-height: 647px;
}
.cart .zhanwei {
  height: 64px;
}
.cart .shipTitle {
  background-color: #fef8d8;
  color: #e29d29;
  font-size: 12px;
  height: 35px;
  line-height: 35px;
  margin-bottom: 10px;
  text-align: center;
}
.cart .mg50 {
  margin-bottom: 100px !important;
}
.cart .freecost {
  height: 30px;
  line-height: 30px;
  background-color: #fef9dc;
  color: #e7983e;
  font-size: 12px;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 1;
  text-indent: 16px;
}
.cart .freecost .g {
  position: absolute;
  right: 16px;
}
.cart .shop {
  margin-bottom: 10px;
  background: #fff;
}
.cart .shop .shopname {
  height: 40px;
  line-height: 40px;
  font-family: PingFangSC-Light;
  font-size: 13px;
  color: #3A3A3A;
  border: 1px solid #ECECEC;
  padding: 0 16px;
  display: block;
}
.cart .shop .shopname .righticon {
  background: url("https://cdn.youlianyc.com/image/static/f9c69f29d122a91716d44d9f0527fd3f.png") right center no-repeat #fff;
  background-size: 16px;
}
.cart .shop .shopname .righticon .iconcheck {
  display: inline-block;
  width: 30px;
}
.cart .shop .shopname img {
  vertical-align: middle;
}
.cart .shop .lineTop {
  border-top: 1px solid #eee;
  margin-top: 16px;
}
.cart .shop .promotion {
  height: 44px;
  line-height: 44px;
  padding: 0 16px;
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #333333;
  border-bottom: 1px solid #eee;
}
.cart .shop .promotion .tag {
  display: inline-block;
  height: 20px;
  line-height: 20px;
  border: 1px solid #B34852;
  font-family: PingFangSC-Regular;
  font-size: 10px;
  color: #B34852;
  padding: 0 3px;
}
.cart .shop .product {
  min-height: 140px;
  background-color: #fff;
  padding: 0 16px 16px 16px;
}
.cart .shop .product .p-detail {
  position: relative;
}
.cart .shop .product .p-detail .checkbtn {
  position: absolute;
  top: 61px;
}
.cart .shop .product .p-detail .plink {
  position: absolute;
  left: 23px;
  top: 22px;
}
.cart .shop .product .p-detail .plink .picture {
  width: 96px;
  height: 96px;
}
.cart .shop .product .p-detail .product-detail {
  width: 58%;
  margin-left: 5px;
  position: absolute;
  left: 120px;
  top: 22px;
}
.cart .shop .product .p-detail .product-detail .title {
  font-size: 14px;
  color: #3A3A3A;
  height: 20px;
  line-height: 20px;
  width: 90%;
  display: block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.cart .shop .product .p-detail .product-detail .title .giveaway {
  border: 1px solid #D69867;
  display: inline-block;
  width: 34px;
  height: 16px;
  line-height: 16px;
  text-align: center;
  font-size: 10px;
  color: #D69867;
  margin-right: 6px;
  vertical-align: top;
}
.cart .shop .product .p-detail .product-detail .delete {
  position: absolute;
  right: 0;
  top: 0;
  border: 1px solid #3A3A3A;
  font-family: PingFangSC-Light;
  font-size: 11px;
  color: #3A3A3A;
  background-color: #fff;
  width: 18px;
  height: 18px;
  line-height: 18px;
  text-align: center;
  padding: 0;
}
.cart .shop .product .p-detail .product-detail .sku {
  font-size: 12px;
  color: #888888;
  margin: 4px 0;
}
.cart .shop .product .p-detail .product-detail .sku .quantity {
  font-size: 12px;
  color: #888;
  float: right;
}
.cart .shop .product .p-detail .product-detail .decrease {
  font-size: 12px;
  color: #B34852;
  line-height: 20px;
}
.cart .shop .product .p-detail .product-detail .disabled {
  color: #c5c4c3;
}
.cart .shop .product .p-detail .product-detail .product_btn {
  border: 1px solid #3A3A3A;
  border-radius: 1px;
  padding: 0;
  padding: 0 10px;
  height: 20px;
  line-height: 20px;
  text-align: center;
  font-family: PingFangSC-Regular;
  font-size: 11px;
  color: #3A3A3A;
  background: #fff;
  float: right;
}
.cart .shop .product .p-detail .product-detail .p-qu {
  font-size: 12px;
  color: #3A3A3A;
}
.cart .shop .product .p-detail .product-detail .p-qu .total_price {
  font-family: DINMittelschriftStd;
  font-size: 14px;
  color: #3A3A3A;
  margin-top: 15px;
}
.cart .shop .product .p-detail .product-detail .quantity {
  height: 28px;
  line-height: 28px;
  background: none;
  font-family: DINMittelschriftStd;
  font-size: 14px;
  color: #3A3A3A;
  position: absolute;
  right: 0;
  bottom: -5px;
}
.cart .shop .product .p-detail .product-detail .quantity .num {
  width: 30px;
  display: inline-block;
  background: none;
  text-align: center;
  border: none;
  font-family: DINMittelschriftStd;
  font-size: 14px;
  color: #3A3A3A;
}
.cart .shop .product .p-detail .product-detail .quantity .add, .cart .shop .product .p-detail .product-detail .quantity .subtract {
  height: 26px;
  width: 26px;
  display: inline-block;
  text-align: center;
  line-height: 26px;
}
.cart .line {
  background: url("//qianka-fx.oss-cn-hangzhou.aliyuncs.com/kagou_app/images/911982b34595339962aee5ed538ebfea95bdb664.png") center bottom no-repeat;
  background-size: 40px;
  margin: 0 auto;
}
.cart .lose {
  background-color: #f5f5f5;
}
.cart .lose .losep {
  position: relative;
}
.cart .lose .shopname {
  background: #fff;
}
.cart .lose .delproduct {
  position: absolute;
  right: 0;
  top: 8px;
  padding: 0 8px;
  height: 24px;
  line-height: 24px;
  text-align: center;
  background: #3A3A3A;
  font-family: PingFangSC-Light;
  font-size: 11px;
  color: #FFFFFF;
  border: none;
}
.cart .lose .product {
  background: #fafafa;
  margin-bottom: 10px;
}
.cart .lose .product .sellout {
  position: absolute;
  right: 0;
  top: 45px;
  width: 87px;
}
.cart .lose .product .p-detail .plink {
  left: 0;
}
.cart .lose .product .p-detail .product-detail {
  left: 101px;
}
.cart .lose .product .p-detail .product-detail .p-qu {
  color: #888;
}
.cart .lose .product .p-detail .product-detail .p-qu .total_price {
  color: #888;
}
.cart .fix-bottom {
  position: fixed;
  bottom: 0;
  width: 100%;
}
.cart .fix-bottom .y518 {
  background: url("http://cdn.youlianyc.com/image/static/9c86f339e8f7164e18fd4987151fb8e43ef2e0a6.jpg!1125") 0 0 no-repeat;
  width: 100%;
  height: 80px;
  background-size: 100%;
  display: grid;
  align-items: center;
  padding: 10px 0;
  box-sizing: border-box;
  text-indent: 70px;
}
.cart .fix-bottom .y518 .tip {
  font-family: PingFang-SC-Medium;
  font-size: 10px;
  color: #FFFFFF;
  text-align: justify;
}
.cart .fix-bottom .y518 .buy518 {
  height: 80px;
  font-family: PingFang-SC-Medium;
  font-size: 14px;
  color: #FFF5FF;
  text-align: center;
}
.cart .fix-bottom .y518 .buy518 label {
  color: #FF274A;
}
.cart .fix-bottom .bottom-confirm {
  height: 50px;
  z-index: 10;
  line-height: 50px;
  display: -webkit-box;
}
.cart .fix-bottom .price {
  color: #333;
  -webkit-box-flex: 8;
  font-size: 16px;
  background: #fff;
  display: block;
}
.cart .fix-bottom .price .allcheck {
  font-size: 12px;
  display: inline-block;
  vertical-align: top;
  margin-left: 16px;
  font-family: PingFang-SC-Medium;
}
.cart .fix-bottom .price .allcheck .xuanicon {
  vertical-align: middle;
}
.cart .fix-bottom .price .total_price {
  font-size: 10px;
  display: inline-block;
  line-height: 20px;
  float: right;
  margin-top: 15px;
  margin-right: 20px;
  text-align: right;
}
.cart .fix-bottom .price .total_price .weightprice {
  font-family: PingFang-SC-Medium;
  font-size: 16px;
  color: #B34852;
}
.cart .fix-bottom .price .total_price .tp {
  font-family: PingFang-SC-Medium;
  font-size: 10px;
  color: #333333;
}
.cart .fix-bottom .buy {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #FFFFFF;
  background: #B34852;
  display: block;
  -webkit-box-flex: 6;
  text-align: center;
  line-height: 50px;
}
.cart .delectSkuPanel {
  width: 85%;
  border-radius: 5px;
}
.cart .delectSkuPanel .ptitle {
  font-size: 16px;
  color: #3A3A3A;
  padding: 20px 0 10px;
  text-align: center;
}
.cart .delectSkuPanel .icon {
  background: url("http://cdn.youlianyc.com/image/static/2d4f133dfe9dd700001aa7fe639d4099732650a8.jpg") center no-repeat;
  width: 79px;
  height: 54px;
  background-size: 100%;
  margin: 0 auto 15px;
}
.cart .delectSkuPanel .btn {
  height: 50px;
  width: 100%;
  line-height: 50px;
  background-color: #f5f5f5;
  display: -webkit-box;
  text-align: center;
  border-bottom-left-radius: 5px;
  border-bottom-right-radius: 5px;
}
.cart .delectSkuPanel .btn div {
  -webkit-box-flex: 1;
  display: block;
  font-size: 14px;
  color: #3A3A3A;
}
.cart .delectSkuPanel .btn .del {
  font-family: PingFang-SC-Medium;
}
.cart .delectSkuPanel .btn .save {
  font-family: PingFangSC-Light;
}
.cart .cartnull {
  text-align: center;
  padding: 83px 0;
  background-color: #fff;
}
.cart .cartnull .title {
  font-family: PingFangSC-Regular;
  font-size: 18px;
  color: #3A3A3A;
}
.cart .cartnull .subtitle {
  font-family: PingFangSC-Light;
  font-size: 14px;
  color: #C5C4C3;
  height: 36px;
  line-height: 36px;
}
.cart .cartnull .cicon {
  height: 166px;
  margin: 27px auto 45px;
}
.cart .cartnull .shopgo {
  display: block;
  border: 1px solid #3A3A3A;
  width: 130px;
  height: 33px;
  line-height: 33px;
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #3A3A3A;
  text-align: center;
  margin: 0 auto;
}