.fixedTop {
  position: fixed;
  width: 92%;
  background-color: #fff;
  top: 0;
  z-index: 10;
  padding: 5px 4%;
  box-shadow: 0 10px 10px -10px #ccc;
  animation: mymove 2s;
  -webkit-animation: mymove 2s;
}
.fixedTop .tabs .t {
  color: #333;
  padding: 5px 0;
}
.fixedTop .search {
  border: 1px solid #eee;
}
.search {
  background: rgba(255,255,255,0.85);
  border-radius: 100px;
  width: 100%;
  height: 30px;
  line-height: 30px;
  display: block;
  font-family: HelveticaNeue-Bold;
  font-size: 14px;
  color: #999999;
  position: relative;
  text-indent: 10px;
}
.search image {
  width: 15px;
  height: 15px;
  position: absolute;
  right: 15px;
  top: 7px;
}
@keyframes mymove {
  0% {
    top: -30px;
    opacity: 0;
  }

  100% {
    top: 0;
    opacity: 1;
  }

}
.headerIndex {
  background: url("http://cdn.youlianyc.com/image/static/1fcf80293281332e699a21e2844442f5c4cddec3.jpg!1125") 0 0 no-repeat;
