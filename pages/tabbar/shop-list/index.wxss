}

.tabs .selectab {
  animation: .5s opacity2 0s 1;
  -webkit-animation: .5s opacity2 0s 1;
  -moz-animation: .5s opacity2 0s 1;
}

@keyframes opacity2 {
  0% {
    opacity: 0;
    font-size: 13px;
  }

  100% {
    opacity: 1;
    font-size: 20px;
  }

}

@-webkit-keyframes opacity2 {
  0% {
    opacity: 0;
    font-size: 13px;
  }

  100% {
    opacity: 1;
    font-size: 20px;
  }

}
